}
 
.title2 {
  width: 520rpx;
  display: flex;
  align-items: center;
}
 
.ui-uploader-item-icon {
  position: absolute;
  right: 0;
}
 
.ui-uploader {
  width: 215rpx;
  height: 215rpx;
  margin-left: 10rpx;
  margin-right: 20rpx;
  background: rgba(43, 153, 255, 0.089);
  border-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}